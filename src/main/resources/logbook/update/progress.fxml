<StackPane prefHeight="200.0" prefWidth="700.0" styleClass="progressWindow" xmlns:fx="http://javafx.com/fxml" fx:controller="logbook.update.ProgressController">
   <padding>
      <Insets bottom="6.0" left="6.0" right="6.0" top="6.0" />
   </padding>
   <VBox>
      <children>
         <Label text="更新を確認しています" />
         <ListView fx:id="progress" focusTraversable="false" />
      </children>
   </VBox>
   <stylesheets>
      <URL value="@updater.css" />
   </stylesheets>
</StackPane>
